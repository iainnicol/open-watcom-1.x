.extensions:
.extensions: .mif .lst .exe .obj .c .h

../objs.mif : aliases.lst ./aliasgen.exe
    rm -f ../objs.mif ../a/*.asm
    $]@ aliases.lst ../a/ ../objs.mif

./aliasgen.exe : $(lib_misc_dir)/c/aliasgen.c
    $(host_cl) $(host_cl_opts) $<

clean : .symbolic
    rm -f aliasgen.exe *.?bj *.err ../a/*.asm ../objs.mif

realclean : .symbolic clean
    rm -f ../objs.mif ../a/*.asm
