[ DEFAULT ]
# DOS and Windows 3.x
type="e" dir="binw"	usr="wd.exe"		cond="doshost"					where="c jc f77 jf77"		descr="Character mode debugger"			old="bin"
type="e" dir="binw"	usr="wdc.exe"		cond="winhost"					where="c jc f77 jf77"		descr="Character-mode debugger"
type="e" dir="binw"	usr="wdw.exe"		cond="winhost"					where="c jc f77 jf77"		descr="GUI debugger"

# OS/2
type="e" dir="binp"	usr="wd.exe"		cond="os2host"					where="c jc f77 jf77"		descr="Character-mode debugger"
type="e" dir="binp"	usr="wdw.exe"		cond="os2host"					where="c jc f77 jf77"		descr="GUI debugger"

# Windows NT - 386
type="e" dir="binnt"	usr="wd.exe"		cond="wnthost"					where="c jc f77 jf77 ac"	descr="Character mode debugger"
type="e" dir="binnt"	usr="wdw.exe"		cond="wnthost"					where="c jc f77 jf77 ac"	descr="GUI debugger"

# Windows NT - Alpha AXP
type="e" dir="axpnt"	usr="wd.exe"		cond="alphahost"				where="ac"			descr="Character mode debugger"
type="e" dir="axpnt"	usr="wdw.exe"		cond="alphahost"				where="ac"			descr="GUI debugger"

# Linux - 386
# OS/2
type="e" dir="binl"	usr="wd"		cond="lnxhost"					where="c jc f77 jf77"		descr="Character-mode debugger"

