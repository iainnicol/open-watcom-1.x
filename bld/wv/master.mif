$(exe_name) : eraseall.obj $(resfile) fingmsg.obj $(objs) $(sysobjs) $(libs) $(lnk)
        $(linker) $(lflags) name $^@ @$(lnk) $(endsect)
!ifeq display char
        wstrip -q -a -r $^@ . $(resfile)
!else ifeq host_OS win
        $(rc) -k $(rc_flags) -i="$(rcinclude);$(lang_root)/h/win" $(resfile) $^@
!else ifeq host_OS nt
        $(rc) -k $(rc_flags) -i="$(rcinclude);$(lang_root)/h/nt" $(resfile) $^@
!else ifeq host_OS os2
        $(rc_os2) -p -x $(resfile) $^@
!endif

wd.lib : eraseall.obj $(engine_objs) $(sysobjs)
        @%create tmp.lbc
        @for %i in ($(engine_objs) $(sysobjs)) do @%append tmp.lbc +%i
        $(librarian) -b -n wd.lib @tmp.lbc

$(lnk) : $(__MAKEFILES__)
    %create $(lnk)
    @%append $(lnk) option map,nocache
!ifdef syslibs
    @for %i in ($(syslibs)) do @%append $(lnk) library %i
!endif
    @for %i in ($(libs))    do @%append $(lnk) library %i
    @for %i in ($(sysobjs)) do @%append $(lnk) file %i
    @%append $(lnk) file fingmsg.obj
!ifeq host_OS dos
!ifdef ovl_dos
    @%append $(lnk) begin autosection
!endif
!endif
    @for %i in ($(objs)) do @%append $(lnk) file %i

