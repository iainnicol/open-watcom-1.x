        wcmdlist.obj &
        wcolrdlg.obj &
        wcombox.obj &
        wcontrol.obj &
        wdialog.obj &
        weditbox.obj &
        weditdlg.obj &
        wfile.obj &
        wfiledlg.obj &
        wfilelst.obj &
        wfilenam.obj &
        wgroupbx.obj &
        whotspot.obj &
        winflist.obj &
        winpdlg.obj &
        wlistbox.obj &
        wmdichld.obj &
        wmdiwndw.obj &
        wmenu.obj &
        wmenuitm.obj &
        wmenuobj.obj &
        wmenusep.obj &
        wmetrics.obj &
        wmodel.obj &
        wmsgdlg.obj &
        wname.obj &
        wobjfile.obj &
        wobjmap.obj &
        wpaint.obj &
        wpickbox.obj &
