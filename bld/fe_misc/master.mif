proj_name = fe_misc

!include cproj.mif
!include deftarg.mif
!include defrule.mif

.c: ../c
.h: ../h

all: genverrc.exe

genverrc.exe: genverrc.c
!ifeq verbose 1
        $(bld_cl) $(wcl_util_opts) -D_VERSION=$(bld_ver) $<
!else
        @echo cl -D_VERSION=$(bld_ver) $@
	@$(bld_cl) $(wcl_util_opts) -D_VERSION=$(bld_ver) $<
!endif
