#
# Watcom Help converter (WHPVCT) Makefile
# =======================================
#

proj_name = whpcvt

!include cproj.mif
!include deftarg.mif
!include defrule.mif

inc_dirs = $(whpcvt_dir)\h

.c: ..\c

objs =              &
        whpcvt.obj  &
        rtf.obj     &
        ipf.obj     &
        ib.obj      &
        html.obj

$(proj_name).exe : $(objs)
        $(linker) option quiet $(lflags) name $^. file { $< }
