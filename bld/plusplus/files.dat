usr="wppi8601.int"	dir="binp/dll" 	cond="os2host tools16" 		where="  jc"

#  386 target
usr="wpp386.exe" 	dir="binp" 	cond="os2host" 		where="c jc"
usr="wppd386.dll" 	dir="binp/dll" 	cond="os2host" 		where="c jc"
usr="wpp38601.int" 	dir="binp/dll" 	cond="os2host" 		where="  jc"

