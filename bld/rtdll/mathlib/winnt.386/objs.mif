pfx__ =$(mathlib_dir)
__ifx__ =/library/$(system).$(processor)/$(subdir)

dll_libs = $+ &
    $(pfx__)$(__ifx__)dll/math3$(dllname_$(convention)).lib &
    ../../../clib/$(system).386/$(subdir)/clib3$(dllname_$(convention)).lib &
    $-

dll_def = $+ &
    $(pfx__)$(__ifx__)dll/export.def &
    $(pfx__)/export.$(processor) &
    $-

dll_objs = $+ &
    $(clib_dir)/startup/library/$(system).386/$(subdir)/clibdwnt.obj &
    $-

static_objs =

