proj_name = wlink

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs = &
fileio.obj &
keyword.obj &
mem.obj &
ms2wlink.obj &
parsems.obj &
utils.obj

prj = ms2wlink.exe

.c: ..\c

lflags_dos = system dos
lflags_nt = system nt
lflags_os2 = system os2 pmcompat op stack=8k

$(prj) : $(objs)
        $(linker) $(lflags_$(host_os)) n $(prj) op map f {$(objs)}

