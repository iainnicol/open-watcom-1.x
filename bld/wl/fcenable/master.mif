proj_name = wlink

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs  = &
!ifeq host_OS os2
spawn.obj &
!endif
fcenable.obj &
mem.obj &
records.obj &
wildargv.obj

!ifdef on_build_machine
copts += -zq
!endif

prj = fcenable.exe

.c: ..\c;$(lang_root)\src\startup

lflags_dos = system dos
lflags_nt  = system nt
lflags_os2 = system os2 pmcompat

$(prj) : $(objs)
        $(linker) $(lflags_$(host_OS)) n $(prj) op map f {$(objs)}

