WINDOWS = 1
!include wres.mif

!ifneq host_CPU axp
viper_lib       = $(viper_dir)\mviper\$(host_OS)\mviper.lib
!else
viper_lib       = $(viper_dir)\mviper\axp\mviper.lib
!endif

wclass_dir_nt_386       = win\nt
wclass_dir_nt_axp       = win\axp
wclass_dir_nt   = $(wclass_dir_nt_$(host_CPU))
wclass_dir_os2  = pm\obj32
wclass_dir_win  = win\obj

wclass_lib      = $(wclass_dir)\$(wclass_dir_$(host_OS))\wclass.lib

!ifeq host_CPU axp
gui_lib         = $(gui_dir)/win/ntaxp/gui.lib
!else
!ifeq host_OS nt
gui_lib         = $(gui_dir)/win/nt386/gui.lib
!else
!ifeq host_OS os2
gui_lib         = $(gui_dir)/win/os2386/gui.lib
!else
gui_lib         = $(gui_dir)/win/wini86/gui.lib
!endif
!endif
!endif

!ifneq host_OS os2
LIBS = $(wres_lib) $(viper_lib) $(wclass_lib) $(gui_lib)
!else
LIBS = $(viper_lib) $(wclass_lib) $(gui_lib)
!endif
