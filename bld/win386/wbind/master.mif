proj_name = wbind

!include cproj.mif
!include deftarg.mif
!include defrule.mif

.c : ..

extra_c_flags =-s-fpc
extra_c_flags_dos =-3-d_DOS
extra_c_flags_os2 =-3
extra_l_flags_nt = option stack=12k,map
extra_l_flags_dos = option stack=8k,map
extra_l_flags_os2 = option stack=12k,heap=8k,newfile,map

wbind.exe: wbind.obj
    $(linker) $(lflags) file wbind

