.CONTINUE
.IGNORE

!ifndef testasm
testasm=wasm
!endif 

all : clean $(objs) $(dev_objs) .SYMBOLIC

$(objs) : .SYMBOLIC
	@echo $^@ ================================================================
	@$(testasm) -q -zld $(asm_flags_$^@) $^@.asm >$^@.tmp 2>&1
        @wmake -h -f ../check.mif name=$^@

$(dev_objs) : .SYMBOLIC
	@echo .
	@echo ==============================================================
	@echo DEVELOPMENT - $^@.asm 
	@echo ==============================================================
	@echo .
	@$(testasm) -q -zld $(asm_flags_$^@) $^@.asm >$^@.tmp 2>&1
        @wmake -h -f ../check.mif name=$^@

clean : .SYMBOLIC
	@rm -f *.obj *.err *.dis *.lst *.tmp mem.trk
