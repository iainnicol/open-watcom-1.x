(MSG_USE_E_BASE+37-1), "-nm=<name>          set module name"
(MSG_USE_E_BASE+38-1), "-nt=<name>          set name of text segment"
(MSG_USE_E_BASE+39-1), "-o                  allow C form of octal constants"
(MSG_USE_E_BASE+40-1), "-zq or -q           operate quietly"
(MSG_USE_E_BASE+41-1), "-?                  print this message"
