.extensions:
.extensions: .lib .obj .cpp

template = &
wcexcept.obj &
wchash.obj &
wclist.obj &
wcskip.obj

lbc = $(%RAMDISK)\tmp.lbc

..\$(contain_lib) : $(template)
    @%create $(lbc)
    @for %i in ($(template)) do @%append $(lbc) +%i
    wlib /b /c /n /q /t $^@ @$(lbc)

.cpp:..\..\cpp
.cpp.obj: .AUTODEPEND
    $(ppcompile) -xs-zm-zld @compflags $[@
