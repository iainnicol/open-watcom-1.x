!include deftarg.mif

.extensions:
.extensions: .lib .obj .cpp

template = &
wcexcept.obj &
wchash.obj &
wclist.obj &
wcskip.obj

lbc = tmp.lbc

../$(contain_lib) : $(template)
    @%create $(lbc)
    @for %i in ($(template)) do @%append $(lbc) +%i
    wlib -b -c -n -q -t $^@ @$(lbc)

.cpp: ../../cpp
.cpp.obj: .AUTODEPEND
    $(ppcompile) -fo=.obj -xs-zm-zld $(compflags) $[@

