proj_name = edbind
name = edbind

.mif : $(mif_path)
!include cproj.mif
!include defrule.mif
!include deftarg.mif

!ifdef tools_9_0
linker = wlink9xx
!endif

.c : ..

extra_c_flags = -s
extra_c_flags_386 = -fpc

inc_dirs = ..\..\h;$(watcom_dir)\h

extra_l_flags_dos = op quiet
extra_l_flags_os2 = op stack=12k,heap=8k,quiet
extra_l_flags_nt = op stack=20k,caseexact,quiet
extra_l_flags = name $(name)

edbind.exe: main.obj
        $(linker) $(lflags) file main.obj name $^@
