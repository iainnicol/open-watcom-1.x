# DIP component of install manifest
[ DEFAULT ]

#	OS/2
usr="codeview.d32"	dir="binp"	cond="os2host"			where="c jc f77 jf77"
usr="dwarf.d32"		dir="binp"	cond="os2host"			where="c jc f77 jf77"
usr="export.d32"	dir="binp"	cond="os2host"			where="c jc f77 jf77"
usr="mapsym.d32"	dir="binp"	cond="os2host"			where="c jc f77 jf77"
usr="watcom.d32"	dir="binp"	cond="os2host"			where="c jc f77 jf77"
#	WINDOWS	
usr="codeview.dll"	dir="binw"	cond="winhost wintarg |"	where="c jc f77 jf77"
usr="dwarf.dll"		dir="binw"	cond="winhost wintarg |"	where="c jc f77 jf77"
usr="export.dll"	dir="binw"	cond="winhost wintarg |"	where="c jc f77 jf77"
usr="mapsym.dll"	dir="binw"	cond="winhost wintarg |"	where="c jc f77 jf77"
usr="watcom.dll"	dir="binw"	cond="winhost wintarg |"	where="c jc f77 jf77"
#	DOS	
usr="codeview.dip"	dir="binw"	cond="winhost doshost | dostarg |"	where="c jc f77 jf77"
usr="dwarf.dip"		dir="binw"	cond="winhost doshost | dostarg |"	where="c jc f77 jf77"
usr="export.dip"	dir="binw"	cond="winhost doshost | dostarg |"	where="c jc f77 jf77"
