usr="watcom.dip"	dir="binw"	cond="winhost doshost | dostarg |"	where="c jc f77 jf77"
#	NT 
usr="codeview.dll"	dir="binnt"	cond="wnthost wnttarg |"	where="c jc f77 jf77 ac"
usr="dwarf.dll"		dir="binnt"	cond="wnthost wnttarg |"	where="c jc f77 jf77 ac"
usr="export.dll"	dir="binnt"	cond="wnthost wnttarg |"	where="c jc f77 jf77 ac"
usr="mapsym.dll"	dir="binnt"	cond="wnthost wnttarg |"	where="c jc f77 jf77 ac"
usr="watcom.dll"	dir="binnt"	cond="wnthost wnttarg |"	where="c jc f77 jf77 ac"
#	Alpha NT
usr="codeview.dll"	dir="axpnt"	cond="alphahost alphatarg |"	where="ac"
usr="dwarf.dll"		dir="axpnt"	cond="alphahost alphatarg |"	where="ac"
usr="export.dll"	dir="axpnt"	cond="alphahost alphatarg |"	where="ac"
usr="mapsym.dll"	dir="axpnt"	cond="alphahost alphatarg |"	where="ac"
usr="watcom.dll"	dir="axpnt"	cond="alphahost alphatarg |"	where="ac"
