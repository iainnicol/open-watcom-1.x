clean : .symbolic
!ifndef clean_dir
clean_dir = .
!endif
        @rm -f $(clean_dir)/*.ex? $(clean_dir)/*.?bj $(clean_dir)/*.map
        @rm -f $(clean_dir)/*.lst $(clean_dir)/*.smp $(clean_dir)/*.lnk
        @rm -f $(clean_dir)/*.lbc $(clean_dir)/*.lib $(clean_dir)/*.err
        @rm -f $(clean_dir)/*.sym
